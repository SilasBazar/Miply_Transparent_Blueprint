{
  "name": "miply-transparent-blueprint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:scss": "sass assets/scss/main.scss src/css/main.css --no-source-map",
    "build:css": "tailwindcss -i ./src/css/main.css -o ./dist/assets/css/main.css --minify",
    "build": "npm run build:scss && npm run build:css",
    "watch:scss": "sass --watch assets/scss/main.scss:src/css/main.css --no-source-map",
    "watch:css": "tailwindcss -i ./src/css/main.css -o ./dist/assets/css/main.css --watch",
    "dev": "npm run build:scss && npm run watch:scss"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.4.8",
    "sass": "^1.72.0"
  }
}